{
  "name": "Carma TV Uploader",
  "description": "Automate article Extraction & Upload tool ",
  "version": "1.0",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "background": {
    "scripts": ["background.js"]},
  "permissions": [
    "tabs",
    "https://bodhi.carma.com/*",
    "https://bodhi-staging.carma.com/*"
  
  ]
}